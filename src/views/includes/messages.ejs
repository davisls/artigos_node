<% if(errors.length > 0) { %>
  
<div class="msg">
    <div class="msg-erro">
        <% errors.forEach(erro => { %>
            <%=erro%> <br>
        <% }); %>
    </div>
</div>    

<% } %>

<% if(success.length > 0) { %>
  
    <div class="msg">
        <div class="msg-success">
            <% success.forEach(succes => { %>
                <%=succes%> <br>
            <% }); %>
        </div>
    </div>    
    
    <% } %>
